# Number of virtual machines to create
VM_COUNT=3

# Name of the base box (Ubuntu or other)
VM_BOX=ubuntu/bionic64

# Amount of RAM allocated to each VM (in MB)
VM_RAM=512

# Number of CPUs allocated to each VM
VM_CPU=1

# Prefix for VM names (e.g., vm1, vm2, vm3)
VM_PREFIX=vm

# Base IP address range for the private network (adjust to your local network)
VM_BASE_IP=192.168.56
